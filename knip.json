  "entry": [
    "src/routes/**/*.tsx",
    "src/routes/**/*.ts",
    "src/entry-client.tsx",
    "src/entry-server.tsx",
    "src/main.tsx",
    "vite.config.ts",
    "eslint.config.js",
    "prettier.config.js",
    "tailwind.config.js",
    "postcss.config.js"
